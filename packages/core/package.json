    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "scripts": {
        "build": "tsup --clean --dts",
        "dev": "tsup --watch --sourcemap",
        "dev:dts": "tsup --watch --dts",
        "bump": "bumpp",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
